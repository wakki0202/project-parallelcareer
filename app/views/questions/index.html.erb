


<div class="provider-index">
<div class="side">
<%= render 'providers/side' %>
</div>

<div class="bg_pattern Paper_v2">
<div class="providers">

<h1>お問い合わせ一覧</h1>

<table>
  <thead>
    <tr>
      <th>名前</th>
      <th>お問い合わせ内容</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @questions.each do |question| %>
      <tr>
        <td><%= question.name %></td>
        <td><%= question.content %></td>
        <td><%= link_to '詳細を見る', question %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
</div>
</div>