   <table class="table userTable ">
          <thead class="heading">
            <tr class="text-left">
              <th>タイトル</th>
              <th>作成日</th>
              <th></th>
            </tr>
          </thead>

          <tbody>
            <% @news.each do |news| %>
              <tr class="text-left">
                <td><%= news.title %></td>
                <td><%= news.created_at.strftime('%Y年%m月%d日') %></td>
                <td><%= link_to '詳細', news,class:"toShow py-1 px-3 text-decoration-none" %></td>
                
              </tr>
            <% end %>
          </tbody>
        </table>