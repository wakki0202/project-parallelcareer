<%= render '/layouts/header' %>


<body class="userConfirm">
<h1>ユーザープロフィール</h1>

<table>
  <tr>
    <th>ユーザー名</th>
    <td><%= current_user.username %></td>
  </tr>
  <tr>
    <th>メールアドレス</th>
    <td><%= current_user.email %></td>
  </tr>
  <tr>
    <th>電話番号</th>
    <td><%= current_user.phonenumber %></td>
  </tr>
</table>
</body>